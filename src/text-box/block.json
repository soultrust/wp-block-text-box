{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "blocks-course/text-box",
	"version": "0.1.0",
	"title": "Text Box",
	"category": "text",
	"icon": "text-page",
	"description": "A box of textx.",
	"keywords": ["text", "paragraph", "box"],
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "text-box",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"text": {
			"type": "string",
			"source": "html",
			"selector": "h4"
		},
		"alignment": {
			"type": "string",
			"default": "left"
		}
	}
}
